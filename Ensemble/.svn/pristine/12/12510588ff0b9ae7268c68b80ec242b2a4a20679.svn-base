<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/billing_account_main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="truecorp.ensemble.BillingAccountActivity">

    <include layout="@layout/title"/>

    <ImageButton
        android:id="@+id/searchButtonTop"
        android:layout_width="53dp"
        android:layout_height="50dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/backbtn"
        android:contentDescription="open search"
        android:scaleX="0.8"
        android:scaleY="0.8"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/icon_search_white"
        tools:visibility="visible" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="48dp"
        android:background="@color/white"
        android:orientation="vertical"
        android:weightSum="100"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/search_panel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:layout_weight="80"
            android:gravity="center_vertical"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_height="100dp"
            tools:layout_weight="5">


            <SearchView
                android:id="@+id/searchBillView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/shape_radius20_red_gray"
                android:scaleX="1"

                android:scaleY="1"
                android:visibility="visible"


                >

            </SearchView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="right"
                android:orientation="horizontal"
                android:weightSum="100"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    style="@style/spinner_item"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_vertical"
                    android:text="Search by "
                    android:textColor="@android:color/black" />

                <Spinner
                    android:id="@+id/search_crytiria"

                    android:layout_width="170dp"
                    android:layout_height="45dp"
                    android:layout_marginEnd="0dp"
                    android:theme="@style/dropdown"
                    android:visibility="visible"
                    tools:layout_editor_absoluteX="8dp"
                    tools:layout_editor_absoluteY="159dp" />
                oo
                <!--<ImageView-->

                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_marginEnd="0dp"-->
                <!--android:layout_marginStart="-55dp"-->
                <!--android:layout_weight="10"-->
                <!--android:scaleX=".5"-->
                <!--android:scaleY=".5"-->
                <!--android:src="@android:drawable/arrow_down_float" />-->


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:weightSum="100"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <Button
                    android:id="@+id/search_center_btn"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/onbluebtntouch"
                    android:text="Search"
                    android:textColor="@color/white" />

            </LinearLayout>

        </LinearLayout>

