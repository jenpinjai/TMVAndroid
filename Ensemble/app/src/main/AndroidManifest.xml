<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="truecorp.ensemble">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".JobMonitorActivity"
            android:label="@string/title_activity_job_monitor" />
        <activity android:name=".BillingAccountActivity" />
        <activity android:name=".AdjustmentActivity" />
        <activity
            android:name=".FileControlActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".CreatePaymentActivity" />
        <activity android:name=".SearchBANForCreatePaymentActivity"></activity>
    </application>

</manifest>